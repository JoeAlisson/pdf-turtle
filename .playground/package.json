    "uuid": "^11.1.0",
    "vue": "^3.4.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@quasar/vite-plugin": "^1.9.0",
    "@types/file-saver": "^2.0.7",
    "@types/json-schema": "^7.0.15",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/cli-plugin-router": "~5.0.8",
    "eslint": "^9.29.0",
