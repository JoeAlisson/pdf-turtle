    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "swagger-axios-codegen": "^0.17.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.10"
  }
